<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhuofengyuan.wechat.shop.mapper.CartItemMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.zhuofengyuan.wechat.shop.entity.CartItem">
                <result column="id" property="id" />
                    <result column="cartid" property="cartid" />
                    <result column="price" property="price" />
                    <result column="productid" property="productid" />
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
                id,
            cartid, price, productid
        </sql>

</mapper>